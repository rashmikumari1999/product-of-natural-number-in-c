#include <stdio.h>

int main()
{
    int i,N,p=1;
    printf("/n enter a natural number to calculate their product");
    scanf("%d",&N);
    for(i=1;i<=N;i++)
    p=p*i;
    printf("calculate the number of product is%d",p);
}
